<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.workoutwarrior.workoutwarrior"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="28"
        android:targetSdkVersion="33" />

    <uses-permission android:name="android.permission.INTERNET" />

    <permission
        android:name="com.workoutwarrior.workoutwarrior.DYNAMIC_RECEIVER_NOT_EXPORTED_PERMISSION"
        android:protectionLevel="signature" />

    <uses-permission android:name="com.workoutwarrior.workoutwarrior.DYNAMIC_RECEIVER_NOT_EXPORTED_PERMISSION" />

    <application
        android:name="com.workoutwarrior.workoutwarrior.WorkoutWarriorApplication"
        android:allowBackup="true"
        android:appComponentFactory="androidx.core.app.CoreComponentFactory"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:debuggable="true"
        android:extractNativeLibs="false"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:testOnly="true"
        android:theme="@style/Theme.WorkoutWarrior"
        android:usesCleartextTraffic="true" >
        <activity
            android:name="com.workoutwarrior.workoutwarrior.UpdateUserActivity"
            android:exported="false"
            android:parentActivityName="com.workoutwarrior.workoutwarrior.UserInfoActivity" />
        <activity
            android:name="com.workoutwarrior.workoutwarrior.UserInfoActivity"
            android:exported="false"
            android:parentActivityName="com.workoutwarrior.workoutwarrior.HomeScreen" />
        <activity
            android:name="com.workoutwarrior.workoutwarrior.CreateUserActivity"
            android:exported="false" />
        <activity
            android:name="com.workoutwarrior.workoutwarrior.WelcomeActivity"
            android:exported="false" />
        <activity
            android:name="com.workoutwarrior.workoutwarrior.HomeScreen"
            android:exported="false" />
        <activity
            android:name="com.workoutwarrior.workoutwarrior.ExerciseInputActivity"
            android:exported="false"
            android:parentActivityName="com.workoutwarrior.workoutwarrior.HomeScreen" />
        <activity
            android:name="com.workoutwarrior.workoutwarrior.ExerciseRecyclerActivity"
            android:exported="false"
            android:parentActivityName="com.workoutwarrior.workoutwarrior.ExerciseInputActivity" />
        <activity
            android:name="com.workoutwarrior.workoutwarrior.ExerciseDetail"
            android:exported="false" />
        <activity
            android:name="com.workoutwarrior.workoutwarrior.WorkoutListActivity"
            android:exported="false"
            android:parentActivityName="com.workoutwarrior.workoutwarrior.WorkoutSaveActivity" />
        <activity
            android:name="com.workoutwarrior.workoutwarrior.WorkoutSaveActivity"
            android:exported="false"
            android:parentActivityName="com.workoutwarrior.workoutwarrior.HomeScreen" />
        <activity
            android:name="com.workoutwarrior.workoutwarrior.MainActivity"
            android:exported="true" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <provider
            android:name="androidx.startup.InitializationProvider"
            android:authorities="com.workoutwarrior.workoutwarrior.androidx-startup"
            android:exported="false" >
            <meta-data
                android:name="androidx.emoji2.text.EmojiCompatInitializer"
                android:value="androidx.startup" />
            <meta-data
                android:name="androidx.lifecycle.ProcessLifecycleInitializer"
                android:value="androidx.startup" />
        </provider>
    </application>

</manifest>